[auth]
type = htpasswd
htpasswd_filename = /var/lib/radicale/htpasswd
htpasswd_encryption = bcrypt

[storage]
filesystem_folder = /var/lib/radicale/collections

[privacy]
type = database
database_path = /var/lib/radicale/privacy.db

[rights]
type = authenticated

[logging]
level = info
mask_passwords = True
